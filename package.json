{
  "name": "dimigoin-back-v4",
  "version": "4.1.0",
  "description": "dimigoin backend version 4",
  "author": "Yeonjun Lee <yeonfish6040@gmail.com>",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "biome": "biome check --write",
    "build": "nest build -b swc",
    "format": "biome format --write",
    "predev": "doppler run -- bun migration:dev",
    "prestart:prod": "bun migration:prod",
    "dev": "doppler run -- bun --watch src/main.ts",
    "start:prod": "bun dist/src/main.js",
    "lint": "biome lint --write",
    "truncate": "bun run src/scripts/truncate-all.ts",
    "truncate:test": "NODE_ENV=test bun run src/scripts/truncate-all.ts",
    "truncate:dev": "NODE_ENV=dev bun run src/scripts/truncate-all.ts",
    "typeorm": "bun run ./node_modules/typeorm/cli.js",
    "typeorm:prod": "NODE_ENV=prod bun run ./node_modules/typeorm/cli.js",
    "typeorm:dev": "NODE_ENV=dev bun run ./node_modules/typeorm/cli.js",
    "migration": "$(bun truncate && rm src/migrations/* && bun typeorm migration:generate src/migrations/InitSchema -d src/scripts/data-source.ts && bun typeorm migration:run -d src/scripts/data-source.ts) || true",
    "migration:prod": "rm -rf dist/src/migrations/*; (bun typeorm:prod migration:generate src/migrations/InitSchema -d src/scripts/data-source.ts && bun typeorm:prod migration:run -d src/scripts/data-source.ts) || true",
    "migration:dev": "rm -rf ./src/migrations/*; (bun typeorm:dev migration:generate src/migrations/InitSchema -d src/scripts/data-source.ts && bun typeorm:dev migration:run -d src/scripts/data-source.ts) || true",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:verbose": "bun test --verbose"
  },
  "dependencies": {
    "@date-fns/tz": "^1.4.1",
    "@fastify/compress": "^8.3.1",
    "@fastify/cookie": "^11.0.2",
    "@fastify/jwt": "^10.0.0",
    "@fastify/multipart": "^9.3.0",
    "@fastify/static": "^9.0.0",
    "@googleapis/fcm": "^9.0.1",
    "@googleapis/youtube": "^30.2.0",
    "@nestjs/cache-manager": "^3.1.0",
    "@nestjs/common": "^11.1.11",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.11",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/platform-fastify": "^11.1.11",
    "@nestjs/schedule": "^6.1.0",
    "@nestjs/swagger": "^11.2.4",
    "@nestjs/typeorm": "^11.0.0",
    "cache-manager": "^7.2.8",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "date-fns": "^4.1.0",
    "fastify": "^5.6.2",
    "google-auth-library": "^10.5.0",
    "jwks-rsa": "^3.2.0",
    "nestjs-swagger-dto": "^3.8.5",
    "pg": "^8.16.3",
    "rxjs": "^7.8.2",
    "swagger-themes": "^1.4.3",
    "typeorm": "0.3.28"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@nestjs/cli": "^11.0.14",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.11",
    "@swc/cli": "^0.7.9",
    "@swc/core": "^1.15.8",
    "@types/bun": "^1.3.5",
    "typescript": "^5.9.3"
  }
}
