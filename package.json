{
  "name": "dimigoin-back-v4",
  "version": "0.0.1",
  "description": "dimigoin backend version 4",
  "author": "Yeonjun Lee <yeonfish6040@gmail.com>",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "predev": "npm run migration:dev",
    "prestart:prod": "npm run migration:prod",
    "dev": "NODE_ENV=dev nest start --watch",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "pretest:e2e": "npm run migration:test",
    "test:e2e": "jest --config ./test/jest-e2e.json -i",
    "truncate": "npx ts-node -r tsconfig-paths/register src/scripts/truncate-all.ts",
    "truncate:prod": "NODE_ENV=prod npx ts-node -r tsconfig-paths/register src/scripts/truncate-all.ts",
    "truncate:test": "NODE_ENV=test npx ts-node -r tsconfig-paths/register src/scripts/truncate-all.ts",
    "truncate:dev": "NODE_ENV=dev npx ts-node -r tsconfig-paths/register src/scripts/truncate-all.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:prod": "NODE_ENV=prod ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:test": "NODE_ENV=test ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:dev": "NODE_ENV=dev ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration": "$(npm run truncate && rm src/migrations/* && npm run typeorm migration:generate src/migrations/InitSchema -- -d src/scripts/data-source.ts && npm run typeorm migration:run -- -d src/scripts/data-source.ts) || true",
    "migration:prod": "(npm run typeorm:prod migration:generate src/migrations/InitSchema -- -d src/scripts/data-source.ts && npm run typeorm:prod migration:run -- -d src/scripts/data-source.ts) || true",
    "migration:test": "npm run truncate:test || true; ((rm -f src/migrations/* && npm run typeorm:test migration:generate src/migrations/InitSchema -- -d src/scripts/data-source.ts && npm run typeorm:test migration:run -- -d src/scripts/data-source.ts) || true); npm run truncate:test",
    "migration:dev": "(rm -f src/migrations/* && npm run typeorm:dev migration:generate src/migrations/InitSchema -- -d src/scripts/data-source.ts && npm run typeorm:dev migration:run -- -d src/scripts/data-source.ts) || true"
  },
  "dependencies": {
    "@nestjs/axios": "^4.0.0",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.0.12",
    "@nestjs/config": "^4.0.1",
    "@nestjs/core": "^11.0.12",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/microservices": "^11.1.3",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.12",
    "@nestjs/schedule": "^5.0.1",
    "@nestjs/swagger": "^11.1.0",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/websockets": "^11.0.12",
    "axios": "1.7.9",
    "bcrypt": "^5.1.1",
    "cache-manager": "^7.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.7",
    "google-auth-library": "^9.15.1",
    "googleapis": "^144.0.0",
    "helmet": "^8.1.0",
    "import-to-array": "^1.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.48",
    "multer": "^1.4.5-lts.2",
    "nestjs-swagger-dto": "^3.8.4",
    "node-ical": "^0.20.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.14.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "swagger-themes": "^1.4.3",
    "typeorm": "0.3.21",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.9",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^11.0.12",
    "@types/bcrypt": "^5.0.2",
    "@types/cache-manager-redis-store": "^2.0.4",
    "@types/cookie-parser": "^1.4.8",
    "@types/cron": "^2.4.3",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.16",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.13.13",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.9.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.5",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.0",
    "ts-jest": "29.2.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.8.2"
  },
  "jest": {
    "globals": {
      "NODE_ENV": "test"
    },
    "moduleNameMapper": {
      "^src/(.*)": "<rootDir>/../src/$1"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "test",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
