{
  "name": "dimigoin-back-v4",
  "version": "4.1.0",
  "description": "dimigoin backend version 4",
  "author": "Yeonjun Lee <yeonfish6040@gmail.com>",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "biome": "npm run format && npm run lint",
    "build": "nest build",
    "format": "biome format --write",
    "predev": "doppler run -- npm run migration:dev",
    "prestart:prod": "npm run migration:prod",
    "dev": "doppler run -- nest start --watch",
    "start:prod": "node dist/src/main",
    "lint": "biome lint --write",
    "pretest:e2e": "npm run migration:test",
    "test:e2e": "jest --config ./test/jest-e2e.json -i",
    "truncate": "npx ts-node -r tsconfig-paths/register src/scripts/truncate-all.ts",
    "truncate:test": "NODE_ENV=test npx ts-node -r tsconfig-paths/register src/scripts/truncate-all.ts",
    "truncate:dev": "NODE_ENV=dev npx ts-node -r tsconfig-paths/register src/scripts/truncate-all.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:prod": "NODE_ENV=prod ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:test": "NODE_ENV=test ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:dev": "NODE_ENV=dev ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration": "$(npm run truncate && rm src/migrations/* && npm run typeorm migration:generate src/migrations/InitSchema -- -d src/scripts/data-source.ts && npm run typeorm migration:run -- -d src/scripts/data-source.ts) || true",
    "migration:prod": "rm -rf ./src/migrations/*; (npm run typeorm:prod migration:generate src/migrations/InitSchema -- -d src/scripts/data-source.ts && npm run typeorm:prod migration:run -- -d src/scripts/data-source.ts) || true",
    "migration:test": "npm run truncate:test || true; ((rm -f src/migrations/* && npm run typeorm:test migration:generate src/migrations/InitSchema -- -d src/scripts/data-source.ts && npm run typeorm:test migration:run -- -d src/scripts/data-source.ts) || true); npm run truncate:test",
    "migration:dev": "rm -rf ./src/migrations/*; (npm run typeorm:dev migration:generate src/migrations/InitSchema -- -d src/scripts/data-source.ts && npm run typeorm:dev migration:run -- -d src/scripts/data-source.ts) || true"
  },
  "dependencies": {
    "@biomejs/biome": "^2.3.8",
    "@keyv/redis": "^5.1.2",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.0.12",
    "@nestjs/config": "^4.0.1",
    "@nestjs/core": "^11.0.12",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/microservices": "^11.1.3",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.12",
    "@nestjs/schedule": "^6.1.0",
    "@nestjs/swagger": "^11.1.0",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/websockets": "^11.0.12",
    "axios": "1.13.2",
    "bcrypt": "^6.0.0",
    "cache-manager": "^7.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.7",
    "firebase-admin": "^13.5.0",
    "google-auth-library": "^10.5.0",
    "googleapis": "^169.0.0",
    "helmet": "^8.1.0",
    "import-to-array": "^1.0.0",
    "ioredis": "^5.7.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "moment-timezone": "^0.6.0",
    "multer": "2.0.2",
    "nestjs-swagger-dto": "^3.8.4",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.14.1",
    "puppeteer": "^24.19.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "swagger-themes": "^1.4.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typeorm": "0.3.28",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.14",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.0.12",
    "@types/bcrypt": "^6.0.0",
    "@types/cache-manager-redis-store": "^3.0.0",
    "@types/cookie-parser": "^1.4.8",
    "@types/cron": "^2.4.3",
    "@types/express": "^5.0.1",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.16",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.0.3",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.5",
    "jest": "^30.2.0",
    "prettier": "^3.5.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.0",
    "ts-jest": "29.4.6",
    "typescript": "^5.8.2"
  },
  "jest": {
    "globals": {
      "NODE_ENV": "test"
    },
    "moduleNameMapper": {
      "^src/(.*)": "<rootDir>/../src/$1"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "test",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
