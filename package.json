{
  "name": "dimigoin-back-v4",
  "version": "4.1.0",
  "description": "dimigoin backend version 4",
  "author": "Yeonjun Lee <yeonfish6040@gmail.com>",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "biome": "pnpm format && pnpm lint",
    "build": "nest build",
    "format": "biome format --write",
    "predev": "doppler run -- pnpm migration:dev",
    "prestart:prod": "pnpm migration:prod",
    "dev": "doppler run -- nest start --watch",
    "start:prod": "node dist/src/main",
    "lint": "biome lint --write",
    "pretest:e2e": "pnpm migration:test",
    "test:e2e": "jest --config ./test/jest-e2e.json -i",
    "truncate": "npx ts-node -r tsconfig-paths/register src/scripts/truncate-all.ts",
    "truncate:test": "NODE_ENV=test npx ts-node -r tsconfig-paths/register src/scripts/truncate-all.ts",
    "truncate:dev": "NODE_ENV=dev npx ts-node -r tsconfig-paths/register src/scripts/truncate-all.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:prod": "NODE_ENV=prod ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:test": "NODE_ENV=test ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:dev": "NODE_ENV=dev ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration": "$(pnpm truncate && rm src/migrations/* && pnpm typeorm migration:generate src/migrations/InitSchema -d src/scripts/data-source.ts && pnpm typeorm migration:run -d src/scripts/data-source.ts) || true",
    "migration:prod": "rm -rf dist/src/migrations/*; (pnpm typeorm:prod migration:generate dist/src/migrations/InitSchema -d dist/src/scripts/data-source.js && pnpm typeorm:prod migration:run -d dist/src/scripts/data-source.js) || true",
    "migration:test": "pnpm truncate:test || true; ((rm -f src/migrations/* && pnpm typeorm:test migration:generate src/migrations/InitSchema -- -d src/scripts/data-source.ts && pnpm typeorm:test migration:run -d src/scripts/data-source.ts) || true); pnpm truncate:test",
    "migration:dev": "rm -rf ./src/migrations/*; (pnpm typeorm:dev migration:generate src/migrations/InitSchema -d src/scripts/data-source.ts && pnpm typeorm:dev migration:run -d src/scripts/data-source.ts) || true"
  },
  "dependencies": {
    "@date-fns/tz": "^1.4.1",
    "@googleapis/fcm": "^9.0.1",
    "@googleapis/youtube": "^30.1.0",
    "@keyv/redis": "^5.1.5",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/cache-manager": "^3.1.0",
    "@nestjs/common": "^11.1.11",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.11",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/microservices": "^11.1.11",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.11",
    "@nestjs/schedule": "^6.1.0",
    "@nestjs/swagger": "^11.2.3",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/websockets": "^11.1.11",
    "axios": "1.13.2",
    "bcrypt": "^6.0.0",
    "cache-manager": "^7.2.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "cookie-parser": "^1.4.7",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "google-auth-library": "^10.5.0",
    "helmet": "^8.1.0",
    "import-to-array": "^1.0.0",
    "ioredis": "^5.9.0",
    "jsonwebtoken": "^9.0.3",
    "jwks-rsa": "^3.2.0",
    "keyv": "^5.5.5",
    "lodash": "^4.17.21",
    "ms": "^2.1.3",
    "multer": "2.0.2",
    "nestjs-swagger-dto": "^3.8.5",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "swagger-themes": "^1.4.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typeorm": "0.3.28",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@nestjs/cli": "^11.0.14",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.11",
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/lodash": "^4.17.21",
    "@types/ms": "^2.1.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.0.3",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.3",
    "jest": "^30.2.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "29.4.6",
    "typescript": "^5.9.3"
  },
  "jest": {
    "globals": {
      "NODE_ENV": "test"
    },
    "moduleNameMapper": {
      "^src/(.*)": "<rootDir>/../src/$1"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "test",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
